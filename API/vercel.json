{
    "builds": [
        {
            "src": "run.py",
            "use": "@vercel/python",
             "config": {
                "maxLambdaSize": "15mb",
                "includeFiles": ["*.py", "app/**/*"]
              }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "run.py"
        }
    ]
}
